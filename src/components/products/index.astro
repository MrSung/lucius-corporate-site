---
import Product from './product.astro'
import {
  dataSapphireSubstrate,
  dataPss,
  dataSic,
  dataGanSubstrate,
  dataAluminumNitrideSubstrate
} from './data'
---

<Product title='Sapphire substrate' data={dataSapphireSubstrate} />

<Product title='PSS (Patterned Sapphire Substrate)' data={dataPss} />

<Product title='SiC substrate' data={dataSic} />

<Product title='GaN substrate' data={dataGanSubstrate} />

<Product title='Aluminum nitride substrate' data={dataAluminumNitrideSubstrate}
/>
